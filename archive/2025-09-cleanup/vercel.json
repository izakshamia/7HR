{
  "version": 2,
  "builds": [
    {
      "src": "server/card_info_server.py",
      "use": "@vercel/python",
      "config": { "pythonVersion": "3.9" }
    },
    {
      "src": "server/templates/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/card_info_server.py"
    }
  ]
}
